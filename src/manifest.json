  },
  "manifest_version": 2,
  "minimum_chrome_version": "42",
  "name": "Zero Width Shortener",
  "offline_enabled": false,
  "permissions": [
    "activeTab"
  ],
  "short_name": "ZWS",
  "version": "1.0.0"
}
